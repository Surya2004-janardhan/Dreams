apes[0], { autoAlpha: 0, duration: 0.3 }, 18.5).fromTo(facebook, { y: 50, autoAlpha: 0 }, { y: 0, autoAlpha: 1, duration: 0.7, ease: \"power2.out\" }, 19.0).fromTo(shapes[1], { scale: 0, autoAlpha: 0, x: 0, y: 0 }, { scale: 1.5, autoAlpha: 0.7, duration: 1, ease: \"power2.out\", x: \"random(-50, 50)\", y: \"random(-50, 50)\" }, 19.0).to(shapes[1], { autoAlpha: 0, duration: 0.3 }, 21.0).to([cocaCola, facebook], { autoAlpha: 0, duration: 0.5 }, 22.5);tl.fromTo(impactText, { scale: 0.5, autoAlpha: 0, rotation: 10 }, { scale: 1, autoAlpha: 1, rotation: 0, duration: 1, ease: \"elastic.out(1, 0.5)\" }, 23.0).fromTo(shapes[2], { autoAlpha: 0, scale: 0 }, { autoAlpha: 0.8, scale: 1.5, duration: 0.8, ease: \"power2.out\", x: \"random(-50, 50)\", y: \"random(-50, 50)\" }, 23.2).fromTo(shapes[3], { autoAlpha: 0, scale: 0 }, { autoAlpha: 0.8, scale: 1.5, duration: 0.8, ease: \"power2.out\", x: \"random(-50, 50)\", y: \"random(-50, 50)\" }, 23.5).to([impactText, shapes[2], shapes[3]], { autoAlpha: 0, duration: 0.5 }, 28.5);tl.fromTo(conclusionText, { autoAlpha: 0, y: 100 }, { autoAlpha: 1, y: 0, duration: 1, ease: \"power3.out\" }, 29.0).fromTo(shapes, { scale: 0, autoAlpha: 0 }, { scale: 1, autoAlpha: 0.8, duration: 1.5, stagger: 0.2, ease: \"back.out(1.7)\", x: \"random(-100, 100)\", y: \"random(-100, 100)\" }, 29.5).to(shapes, { rotate: '+=360', repeat: -1, duration: 10, ease: \"none\" }, \"<\").to(overlay, { autoAlpha: 0, duration: 0.5 }, 38.0);function updateAnimations(currentTime) {if (tl.progress() === 0 && currentTime > 0) {tl.play();} else {tl.seek(currentTime);}}window.addEventListener('message', (event) => {if (event.data && typeof event.data === 'object' && event.data.type === 'videoEvent') {const { eventName, currentTime, layoutMode } = event.data;if (layoutMode === 'full-html' || layoutMode === 'split' || layoutMode === 'pip-html') {gsap.to(overlay, { autoAlpha: 1, duration: 0.3 });} else {gsap.to(overlay, { autoAlpha: 0, duration: 0.3 });}if (eventName === 'timeup